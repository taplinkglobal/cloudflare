{
  "name": "taplink/cloudflare",
  "description": "PHP binding for v4 of the Cloudflare Client API. Forked and modified by tap.link team",
  "type": "library",
  "version": "1.0.0",
  "require": {
    "guzzlehttp/guzzle": "^7.5.1",
    "php": ">=7.2.5",
    "psr/http-message": "~1.0",
    "ext-json": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^v3.16.0",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^9.6.7"
  },
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Junade Ali",
      "email": "junade@cloudflare.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Taplink\\Cloudflare\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
        "tests/"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
